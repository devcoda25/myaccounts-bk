{
    "name": "myaccounts",
    "version": "1.0.0",
    "description": "EVzone Centralized Identity Provider (IdP) and Wallet Service (NestJS + Fastify)",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.962.0",
        "@aws-sdk/s3-request-presigner": "^3.962.0",
        "@emailjs/nodejs": "^5.0.2",
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^10.0.0",
        "@fastify/formbody": "^8.0.2",
        "@fastify/helmet": "^13.0.2",
        "@fastify/middie": "^9.0.0",
        "@fastify/multipart": "^9.3.0",
        "@fastify/static": "^9.0.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mapped-types": "^2.0.6",
        "@nestjs/platform-fastify": "^10.4.15",
        "@nestjs/swagger": "^8.1.0",
        "@nestjs/throttler": "^6.5.0",
        "@prisma/client": "^6.2.1",
        "@simplewebauthn/server": "^13.2.2",
        "@types/bcrypt": "^6.0.0",
        "@types/nodemailer": "^7.0.4",
        "@types/qrcode": "^1.5.6",
        "@types/uuid": "^10.0.0",
        "@willsoto/nestjs-prometheus": "^6.0.2",
        "africastalking": "^0.7.8",
        "apple-signin-auth": "^2.0.0",
        "argon2": "^0.41.1",
        "bcrypt": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.3",
        "fastify": "^5.0.0",
        "file-type": "^21.3.0",
        "geoip-lite": "^1.4.10",
        "google-auth-library": "^10.5.0",
        "ioredis": "^5.8.2",
        "jose": "^5.9.6",
        "kafkajs": "^2.2.4",
        "nestjs-throttler-storage-redis": "^0.5.1",
        "nodemailer": "^7.0.12",
        "oidc-provider": "^8.4.5",
        "otplib": "^12.0.1",
        "prom-client": "^15.1.3",
        "qrcode": "^1.5.4",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "twilio": "^5.11.1",
        "uuid": "^13.0.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/jest": "^30.0.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.17.0",
        "jest": "^30.2.0",
        "prettier": "^3.4.2",
        "prisma": "^6.2.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "overrides": {
        "fastify": "$fastify",
        "@fastify/middie": "^9.0.0",
        "@fastify/cors": "^10.0.0",
        "@fastify/static": "^9.0.0",
        "@fastify/formbody": "^8.0.2"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>"
        ],
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/src/$1"
        }
    }
}
